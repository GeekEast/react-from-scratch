{
  "name": "react-webpack",
  "version": "0.1.0",
  "description": "```sh mkdir react-webpack && cd react-webpack ```",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest --passWithNoTests",
    "format": "pretty-quick",
    "lint": "eslint ./"
  },
  "author": "James",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekEast/react-from-scratch.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/GeekEast/react-from-scratch/issues"
  },
  "homepage": "https://github.com/GeekEast/react-from-scratch#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@types/enzyme": "^3.10.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^3.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-static-stubs": "^0.0.1",
    "prettier": "^1.18.2",
    "react-axe": "^3.2.0",
    "style-loader": "^1.3.0",
    "ts-jest": "^24.0.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "pretty-quick": "^1.11.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm run test"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/enzyme.setup.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
